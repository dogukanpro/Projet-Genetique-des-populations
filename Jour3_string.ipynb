# 1. On définit une séquence d'ADN
sequence = "atgcgtacgtagct"

# 2. On la met en majuscules (standard en bio-info)
sequence_propre = sequence.upper()

# 3. On compte la longueur
longueur = len(sequence_propre)

# 4. On simule une mutation (on remplace un G par un T)
sequence_mutee = sequence_propre.replace("G", "T")

# 5. On affiche tout proprement
print(f"Séquence originale : {sequence_propre}")
print(f"Longueur : {longueur} bases")
print(f"Séquence après mutation : {sequence_mutee}")